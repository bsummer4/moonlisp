(def p (js "console.log(%1)"))
(def a (js "window.alert(%1)"))
(def w (js "document.write(%1)"))
(def mkdom (js "document.createElement(%1)"))
(def gid (js "document.getElementById(%1)"))
(def appendChild (js "(%1).appendChild(%2)"))
(def setInnerText (js "(%1).innerText = %2"))
(def append (λ {x c} [appendChild x c]))
(def withText (λ {x t} (setInnerText x t)))

[w “<div id="sup">sup</div>”]
[a "Hello world!"]
[mkdom "span"]
[append [gid "sup"] [setText [mkdom "span"] " span"]]
